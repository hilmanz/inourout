	background: linear-gradient(to bottom, rgba(0,173,238,1) 0%,rgba(0,140,193,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00adee', endColorstr='#008cc1',GradientType=0 ); /* IE6-9 */
	display:inline-block;
	zoom: 1;
	*display: inline;
	height:30px;
	line-height:30px;
	padding:5px 30px;
	text-align:center;
	color:#005e8a;
	text-shadow:0 0 1px #5cbce0;
