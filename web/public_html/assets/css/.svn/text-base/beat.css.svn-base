	background:url(../img/icon/icon_flag_red.png) no-repeat  -1px -7px;
	padding:0 0 0 25px;
	color:#f20000;
}
/*------------------------------------------------------------------
[4. Nav  / #nav]
*/
#sidebar .widget{
	margin:0;
}
#navigation{
	border-top:solid 1px #4a4b4d;
}
#navigation ul{
	margin:0;
	padding:0;
	border-top:solid 1px #1f2226;
}
#navigation li{
	margin:0;
	padding:0;
	line-height:50px;
	border-bottom:solid 1px #1f2226;
}
#navigation li a{
	display:block;
	line-height:50px;
	border-bottom:solid 1px #4a4b4d;
	padding:0 0 0 30px;
	text-transform:uppercase;
	color:#8f9398;
	position:relative;
	font-size:24px;
}
#navigation li a:hover,#navigation li.active a{
	background: #000;
	color:#FFF;
}
#navigation li.dark-button{
	background:#1d222a;
	padding:10px 0;
}
.arrow-nav{
	background:url(../img/arrow_nav.png) no-repeat;
	display:block;
	position:absolute;
	right:-19px;
	width:19px; height:50px;
	top:0;
	display:none;
	z-index:10;
}
#navigation li a:hover .arrow-nav,#navigation li.active a .arrow-nav{
	display:block;
}
#search-form{
	padding:0 22px;
	border-bottom:1px solid #4A4B4D;
}
.count{
	background:#00aeef;
	display:inline-block;
	zoom: 1;
	*display: inline;
	width:20px; height:20px;
	color:#fff;
	text-align:center;
	border-radius:100px;
	line-height:20px;
	font-size:12px;
	margin:0 0 0 10px;
}
