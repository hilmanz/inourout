/*------------------------------------------------------------------
[1. Global]
*/
@font-face {
    font-family: 'maybebold';
    src: url('../font/maybe-bold-webfont.eot');
    src: url('../font/maybe-bold-webfont.eot?#iefix') format('embedded-opentype'),
         url('../font/maybe-bold-webfont.woff') format('woff'),
         url('../font/maybe-bold-webfont.ttf') format('truetype'),
         url('../font/maybe-bold-webfont.svg#maybebold') format('svg');
    font-weight: normal;
    font-style: normal;

}
body {
	font-family:'maybebold',serif;
	background: url(../img/background-motor.jpg) no-repeat center center fixed; 
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
}
p {
	margin-bottom: 1.4em;
	font: 14px/1.4em 'Titillium Web',"HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
}
#body-container{
	background:url(../img/bg_top.jpg) repeat-x 0 0;
}
#body{
	max-width:1180px;
	margin:0 auto;
	-webkit-box-shadow: 0px 0px 50px rgba(0, 0, 0, 0.85);
	-moz-box-shadow:    0px 0px 50px rgba(0, 0, 0, 0.85);
	box-shadow:         0px 0px 50px rgba(0, 0, 0, 0.85);
}
#bodshadow{
	background:url(../images/shadow.png) bottom center no-repeat;
	padding:0 0 105px 0;
}
a{
	outline:none;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.entry{
	padding:15px;
}
.entry1{
	padding:0 30px;
}
.fl{
	float:left;
}
.fr{
	float:right;
}
