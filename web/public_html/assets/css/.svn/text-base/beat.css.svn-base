/*------------------------------------------------------------------
[3. Info]
*/
#info-user {
	display: table;
	width: auto;
	z-index: 1000;
	margin:30px 10px 0 0;
	list-style: none;
	color:#00aeef;
	float:right;
}
#info-user a{
	text-decoration:none;
}
#info-user a:hover{
	color:#BF1323;
}
#info-menu .info-details {
	display: table-cell;
	vertical-align: bottom;
}
