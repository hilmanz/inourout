	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,173,238,1)), color-stop(100%,rgba(0,140,193,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, rgba(0,173,238,1) 0%,rgba(0,140,193,1) 100%); /* Chrome10+,Safari5.1+ */
